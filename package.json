{
  "name": "@cpro-js/react-core-modules",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "yarn workspaces foreach --all --topological-dev --interlaced run build",
    "test": "vitest",
    "typecheck": "yarn workspaces foreach --all --topological-dev --interlaced run typecheck"
  },
  "dependencies": {
    "import-sort-style-module": "^6.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-package": "^1.3.0"
  },
  "devDependencies": {
    "vitest": "^3.0.5"
  },
  "resolutions": {
    "@storybook/react-vite@npm:8.5.3/@joshwooding/vite-plugin-react-docgen-typescript": "0.5.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "packageManager": "yarn@4.6.0",
  "workspaces": [
    "packages/*"
  ]
}
